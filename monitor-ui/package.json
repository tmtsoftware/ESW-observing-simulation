  "private": true,
  "type": "module",
  "dependencies": {
    "@tmtsoftware/esw-ts": "git+https://github.com/tmtsoftware/esw-ts#f6519b641e7c826450585fd4672c6b96c2b867f8",
    "antd": "4.18.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "scripts": {
    "start": "vite",
    "build": "npm run fix && vite build",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel clean fix",
    "test": "web-test-runner 'test/**/*.test.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix --config .eslintrc.json './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config ./.prettierrc './**/*.{ts,tsx}' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "vite-web-test-runner-plugin": "^0.1.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chai": "^4.3.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@web/test-runner": "^0.13.25",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "vite": "^2.7.12",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "typescript": "^4.5.4"
  }
}
